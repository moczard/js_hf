<div class="review-card">
	<span class="<%= item.rating > 0 ? 'checked' : '' %>">★</span>
	<span class="<%= item.rating > 1 ? 'checked' : '' %>">★</span>
	<span class="<%= item.rating > 2 ? 'checked' : '' %>">★</span>
	<span class="<%= item.rating > 3 ? 'checked' : '' %>">★</span>
	<span class="<%= item.rating > 4 ? 'checked' : '' %>">★</span>
	<div class="comment">
			<%= item.comment %>
	</div>
</div>